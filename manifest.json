{
    "name": "Anitrex",
    "description": "Anime tracking extension that runs AniList API",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "action": {
        "default_popup": "/src/popup.html",
        "default_icon": {
            "16": "/src/img/icon16.png",
            "32": "/src/img/icon32.png",
            "48": "/src/img/icon48.png",
            "128": "/src/img/icon128.png"
        }
    },
    "icons": {
        "16": "/src/img/icon16.png",
        "32": "/src/img/icon32.png",
        "48": "/src/img/icon48.png",
        "128": "/src/img/icon128.png"
    },
    "options_page": "/src/options.html"
}